@article{Croxall2002-ja,
	Abstract = {Recent changes in Antarctic seabird populations may reflect
              direct and indirect responses to regional climate change. The
              best long-term data for high-latitude Antarctic seabirds
              (Ad\'{e}lie and Emperor penguins and snow petrels) indicate that
              winter sea-ice has a profound influence. However, some effects
              are inconsistent between species and areas, some in opposite
              directions at different stages of breeding and life cycles, and
              others remain paradoxical. The combination of recent harvest
              driven changes and those caused by global warming may produce
              rapid shifts rather than gradual changes.},
	Author = {Croxall, J P and Trathan, P N and Murphy, E J},
